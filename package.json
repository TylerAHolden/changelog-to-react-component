{
  "bin": "src/bin.js",
  "bugs": {
    "url": "https://github.com/TylerAHolden/changelog-to-react-component/issues"
  },
  "description": "CLI to convert a CHANGELOG.md following the http://keepachangelog.com convention to a react functional component with permalinks and a GitHub look and feel",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "~5.0.0",
    "github-markdown-css": "~2.10.0",
    "github-slugid": "~1.0.1",
    "highlight.js": "~9.12.0",
    "markdown-it": "~8.4.0",
    "sanitize-html": "~2.3.2",
    "yargs": "~11.0.0"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-prettier": "~2.9.0",
    "eslint-plugin-node": "~5.2.1",
    "standard-version": "^8.0.2"
  },
  "engines": {
    "node": ">= 6.5.0"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "changelog",
    "keepachangelog",
    "markdown",
    "html",
    "github",
    "permalink",
    "react",
    "anchor"
  ],
  "license": "MIT",
  "main": "src",
  "name": "changelog-to-react-component",
  "repository": {
    "type": "git",
    "url": "https://github.com/TylerAHolden/changelog-to-react-component.git"
  },
  "scripts": {
    "eslint": "eslint . --max-warnings 0",
    "release": "standard-version",
    "test": "node src/bin.js --outputDirectoryPath test"
  },
  "version": "0.6.3"
}
